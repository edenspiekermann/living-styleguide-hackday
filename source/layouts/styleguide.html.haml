%html.no-js{lang: "en"}
  / <![endif]
  %head
    %meta{charset: "utf-8"}/
    / Set the viewport width to device width for mobile
    %meta{content: "width=device-width", name: "viewport"}/
    %title= data.page.title || "Elf: Empathy Lab Framework"
    / Included CSS Files
    = stylesheet_link_tag "elf", "app", "styleguide", "coderay"
    = javascript_include_tag "elf/modernizr.elf"
    / IE Fix for HTML5 Tags
    /[if lt IE 9]
      <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  %body{class: page_classes}
    %header.styleguide-header
      Styleguide
    #styleguide
      = partial 'styleguide/nav'
      %h1.title= data.page.title
      = yield
    / Included JS Files
    = javascript_include_tag "jquery.min", |
      "elf/jquery.reveal",                 |
      "elf/jquery.orbit-1.4.0",            |
      "elf/jquery.customforms",            |
      "elf/jquery.placeholder.min",        |
      "elf/jquery.tooltips",               |
      "elf/kss",                           |
      "app"                                |